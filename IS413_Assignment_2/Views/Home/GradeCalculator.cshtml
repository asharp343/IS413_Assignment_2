@{
    ViewBag.Title = "Grade Calculator";
    ViewBag.Header = "Grade Calculator";
}

@model GradeCalculatorModel
@*@model IS413_Assignment_2.Models.GradeCalculatorModel*@



<form method="post">
    <label>Assignments</label>
    <input id="assignments" asp-for="assignments" />
    <br /><br />
    <label>Group Projects</label>
    <input id="groupProjects" asp-for="groupProjects" />
    <br /><br />
    <label>Quizzes</label>
    <input id="quizzes" asp-for="quizzes" />
    <br /><br />
    <label>Exams</label>
    <input id="exams" asp-for="exams" />
    <br /><br />
    <label>INTEX</label>
    <input id="intex" asp-for="intex" />
    <br />
    <br />
    <input type="submit" value="See Grade" id="submitButton" />
    <div asp-validation-summary="All"></div>
</form>

